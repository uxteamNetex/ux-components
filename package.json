{
  "name": "ux-components",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-ux-components-lib": "ng build @netex/ux-components",
    "build-ux-components-themes": "sass-pack -s ./projects/netex/ux-components/src/themes/*.theme.scss -o ./dist/netex/ux-components/themes",
    "build-ux-components-images": "mkdir -p ./dist/netex/ux-components/assets && cp -R ./src/assets/images ./dist/netex/ux-components/assets",
    "build-ux-components-fonts-icons": "mkdir -p ./dist/netex/ux-components/assets && cp -R ./src/assets/fonts ./dist/netex/ux-components/assets",
    "build-ux-components-styles": "sass-pack -s ./projects/netex/ux-components/src/styles/styles.scss -o ./dist/netex/ux-components/styles/",
    "build-ux-components": "rm -rf dist/netex/ux-components && npm run build-ux-components-lib && npm run build-ux-components-themes && npm run build-ux-components-images && npm run build-ux-components-styles && npm run build-ux-components-fonts-icons"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.0",
    "@angular/cdk": "^7.1.0",
    "@angular/common": "^7.1.0",
    "@angular/compiler": "^7.1.0",
    "@angular/core": "^7.1.0",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.1.0",
    "@angular/http": "^7.1.0",
    "@angular/material": "^7.1.0",
    "@angular/platform-browser": "^7.1.0",
    "@angular/platform-browser-dynamic": "^7.1.0",
    "@angular/router": "^7.1.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "ngx-drag-scroll": "^2.1.1",
    "node-sass": "^7.0.3",
    "panzoom": "^7.1.2",
    "rxjs": "^6.3.3",
    "sass": "^1.55.0",
    "sass-pack": "^2.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~7.1.0",
    "@angular/compiler-cli": "^7.1.0",
    "@angular/language-service": "^7.1.0",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~10.12.10",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "ng-packagr": "^4.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
